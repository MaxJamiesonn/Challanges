Names = ["Max","Phelan","Lucas","Charley","Abdul","Sean","Martina","Jag","Aris"]


Check = input("input name: ")

count = 0

Found = False
while Found == False:
    if Names[count] == Check:
        Found = True
        print("The name is on the list")
    elif count >= len(Names)-1 and Found == False:
        print("The name is not on the list")
        break
        
    else:
        count += 1
