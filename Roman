number = int(input("Enter a number : "))
numlist = [int(digit) for digit in str(number)]

2 == ("II")

if number == 1:
  print("I")
if number == 2:
  print("II")
if number == 3:
  print("III") 
if number == 4:
  print("IV")
if number == 5:
  print("V")
if number == 6:
  print("VI")
if number == 7:
  print("VII")
if number == 8:
  print("VIII")
if number == 9:
  print("IX")
if number == 10:
  print("X")
if number > 10:
  res = len(numlist)
  if res == 2:
    print("X",)
